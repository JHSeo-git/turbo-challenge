{
  "name": "admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "vite",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx,.js .",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js .",
    "build:local": "tsc && vite build --mode on-local",
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "clean": "rm -rf ./node_modules && npm run clean:build",
    "clean:build": "rm -rf ./dist && rm -rf ./.turbo",
    "preview": "tsc && vite build --mode on-local && vite preview",
    "dockerize": "docker-compose up -d",
    "clean:dockerize": "docker-compose down && docker rmi turbo-challenge"
  },
  "dependencies": {
    "@repo/ui": "*",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.2.0",
    "vite-plugin-environment": "^1.1.3"
  }
}
